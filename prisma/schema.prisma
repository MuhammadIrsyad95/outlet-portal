generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Location {
  id        Int      @id @default(autoincrement())
  chain     String
  category  String
  name      String
  address   String
  lat       Float?
  lng       Float?
  createdAt DateTime @default(now())
}
